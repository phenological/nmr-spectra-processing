year <- sub("-.*", "", meta$Date)
note <- sprintf("R package version %s", meta$Version)

bibentry(bibtype = "Manual",
         title = "{nmr-spectra-processing}",
         author = c(person("ANPC", "Team")),
         year = 2023,
         note = note,
         url = "https://github.com/phenological/nmr-spectra-processing")
